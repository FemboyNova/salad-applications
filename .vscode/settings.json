{
  "editor.formatOnSave": true,
  "editor.minimap.maxColumn": 120,
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "git.branchProtection": ["master"],
  "search.exclude": {
    "node_modules": true,
    "scripts": true,
    "**/.yarn": true,
    "**/.pnp.*": true,
    "**/npm-debug.log*": true,
    "**/yarn-debug.log*": true,
    "**/yarn-error.log*": true
  }
}
